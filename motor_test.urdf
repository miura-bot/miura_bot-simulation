<robot name="motor_test">
<!-- Materials -->
  <material name="blue">
    <color rgba = "0 0 0.8 1"/>
  </material>
  <material name="red">
    <color rgba = "0.8 0.0 0 1"/>
  </material>
  <material name="green">
    <color rgba = "0.0 0.8 0 1"/>
  </material>
  <material name="purple">
    <color rgba = "0.4 0.1 0.8 1"/>
  </material>
<!-- motor1 -->
  <link name="motor1">
    <visual>
      <origin rpy="0.0 0 0" xyz="0 -6 -5"/>
      <geometry>
        <mesh filename="motor.stl" /> #scale="0.010 0.010 0.010"/>
      </geometry>
      <material name="green"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 -6 -5"/>
      <geometry>
        <mesh filename="motor.stl" /> #scale="0.010 0.010 0.010"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="13.6 0 0" rpy="0 0 0.0"/>
      <mass value="0.330"/>
      <inertia ixx="6.710" ixy="0.0" ixz="0.0" 
      iyy="24.3067968176" iyz="0.0" izz="23.0967968176"/>
    </inertial>
  </link>

   <!-- motor2 -->
  <link name="motor2">
    <visual>
      <origin rpy="0.0 0 0" xyz="0 -6 -5"/>
      <geometry>
        <mesh filename="motor.stl" />#scale="0.010 0.010 0.010"/>
      </geometry>
      <material name="red"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 -6 -5"/>
      <geometry>
        <mesh filename="motor.stl" />#scale="0.010 0.010 0.010"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="13.6 0 0" rpy="0 0 0.0"/>
      <mass value="0"/>
      <inertia ixx="0" ixy="0.0" ixz="0.0" 
      iyy="0" iyz="0.0" izz="0"/>
    </inertial>
  </link>

  <joint name="jointmotor1" type="continuous">
    <parent link="motor1"/>
    <child link="motor2"/>
    <axis xyz="0 1 0"/>
    <origin rpy="0 0 0" xyz="0 0 0"/> 
  </joint>
  
  
  </robot>

 