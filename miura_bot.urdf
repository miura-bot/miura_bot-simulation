<robot name="miura_bot">

  <!-- Materials -->
  <material name="blue">
    <color rgba = "0.0 0.0 1.0 1"/>
  </material>
  <material name="red">
    <color rgba = "1.0 0.0 0.0 1.0"/>
  </material>
  <material name="green">
    <color rgba = "0.0 0.8 0 1"/>
  </material>

  <!-- base_link -->
  <link name="base_link">
    <visual>
      <origin rpy="0 0 0" xyz="-70.9012 -112.5 -6.0"/>
      <geometry>
        <mesh filename="mori-design-2.stl" scale="1.00 1.00 1.00"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0.0 0 0" xyz="-70.9012 -112.5 -6.0"/>
      <geometry>
        <mesh filename="mori-design-2.stl" scale="1.00 1.00 1.00"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <mass value="0.100"/>
      <inertia ixx="6.25e-05" ixy="0.0" ixz="0.0" iyy="0.00025" iyz="0.0" izz="0.00019"/> <!--w=0.15, d=0.086, h=0.01, m=0.1-->
    </inertial>
  </link>

  <!-- wheel_1 -->
  <link name="wheel_1">
    <visual>
      <origin rpy="0.0 0 0" xyz="-8.0 -2.0 -8.0"/>
      <geometry>
        <mesh filename="wheel.stl" scale="1.00 1.00 1.00"/>
