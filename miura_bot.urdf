<robot name="miura_bot">

  <!-- Materials -->
  <material name="blue">
    <color rgba = "0.0 0.0 1.0 1"/>
  </material>
  <material name="red">
    <color rgba = "1.0 0.0 0.0 1.0"/>
  </material>

  <!-- base_link -->
  <link name="base_link">
    <visual>
      <origin rpy="0.0 0 0" xyz="-70.9012 -112.5 -6.0"/>
      <geometry>
        <mesh filename="mori-design-2.stl" scale="1.00 1.00 1.00"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0.0 0 0" xyz="-70.9012 -112.5 -6.0"/>
      <geometry>
        <mesh filename="mori-design-2.stl" scale="1.00 1.00 1.00"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <mass value="0.100"/>
      <inertia ixx="6.25e-05" ixy="0.0" ixz="0.0" iyy="0.00025" iyz="0.0" izz="0.00019"/> <!--w=0.15, d=0.086, h=0.01, m=0.1-->
    </inertial>
  </link>

  <!-- wheel_1 -->
  <link name="wheel_1">
    <visual>
      <origin rpy="0.0 0 0" xyz="-8.0 -2.0 -8.0"/>
      <geometry>
        <mesh filename="wheel.stl" scale="1.00 1.00 1.00"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin rpy="0.0 0 0" xyz="-8.0 -2.0 -8.0"/>
      <geometry>
        <mesh filename="wheel.stl" scale="1.00 1.00 1.00"/>
      </geometry>
    </collision>
  </link>

  <joint name="shaft_1" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_1"/>
    <axis xyz="0 1 0"/>
    <origin rpy="0 0 0" xyz="-64.9519 -7.5 0"/>
  </joint>

  <!-- wheel_2 -->
  <link name="wheel_2">
    <visual>
      <origin rpy="0.0 0.0 0.0" xyz="-8.0 -2.0 -8.0"/>
      <geometry>
        <mesh filename="wheel.stl" scale="1.00 1.00 1.00"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin rpy="0.0 0.0 0.0" xyz="-8.0 -2.0 -8.0"/>
      <geometry>
        <mesh filename="wheel.stl" scale="1.00 1.00 1.00"/>
      </geometry>
    </collision>
  </link>

  <joint name="shaft_2" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_2"/>
    <axis xyz="0 1 0"/>
    <origin rpy="0 0 -1.0472" xyz="-25.9808 -90.0 0"/>
  </joint>

  <!-- wheel_3 -->
  <link name="wheel_3">
    <visual>
      <origin rpy="0.0 0.0 0.0" xyz="-8.0 -2.0 -8.0"/>
      <geometry>
        <mesh filename="wheel.stl" scale="1.00 1.00 1.00"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin rpy="0.0 0.0 0.0" xyz="-8.0 -2.0 -8.0"/>
      <geometry>
        <mesh filename="wheel.stl" scale="1.00 1.00 1.00"/>
      </geometry>
    </collision>
  </link>

  <joint name="shaft_3" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_3"/>
    <axis xyz="0 1 0"/>
    <origin rpy="0 0 0" xyz="64.9519 7.5 0"/>
  </joint>

  <!-- wheel_4 -->
  <link name="wheel_4">
    <visual>
      <origin rpy="0.0 0.0 0.0" xyz="-8.0 -2.0 -8.0"/>
      <geometry>
        <mesh filename="wheel.stl" scale="1.00 1.00 1.00"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin rpy="0.0 0.0 0.0" xyz="-8.0 -2.0 -8.0"/>
      <geometry>
        <mesh filename="wheel.stl" scale="1.00 1.00 1.00"/>
      </geometry>
    </collision>
  </link>

  <joint name="shaft_4" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_4"/>
    <axis xyz="0 1 0"/>
    <origin rpy="0 0 -1.0472" xyz="25.9808 90.0 0"/>
  </joint>

</robot>