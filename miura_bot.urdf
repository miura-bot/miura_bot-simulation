    <collision>
      <origin rpy="0.0 0 0" xyz="-8.0 -2.0 -8.0"/>
      <geometry>
        <mesh filename="wheel.stl" scale="1.00 1.00 1.00"/>
      </geometry>
    </collision>
  </link>

  <joint name="shaft_1" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_1"/>
    <axis xyz="0 1 0"/>
    <origin rpy="0 0 0" xyz="-64.9519 7.5 0"/>
  </joint>

