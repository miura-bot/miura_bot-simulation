<robot name="miura_bot">

  <!-- Materials -->
  <material name="blue">
    <color rgba = "0 0 0.8 1"/>
  </material>

  <!-- base_link -->
  <link name="base_link">
    <visual>
      <origin rpy="0.0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="mori-design-2.stl" scale="1.00 1.00 1.00"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0.0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="mori-design-2.stl" scale="1.00 1.00 1.00"/>
      </geometry>
    </collision>
  </link>

  <!-- wheel1 -->
  <link name="wheel1">
    <visual>
      <origin rpy="0.0 0 0" xyz="-8 0 -8"/>
      <geometry>
        <mesh filename="wheel.stl" scale="1.00 1.00 1.00"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin rpy="0.0 0 0" xyz="-8 0 -8"/>
      <geometry>
        <mesh filename="wheel.stl" scale="1.00 1.00 1.00"/>
      </geometry>
    </collision>
  </link>

  <joint name="joint1" type="continuous">
    <parent link="base_link"/>
    <child link="wheel1"/>
    <axis xyz="0 1 0"/>
    <!-- <limit effort="1000.0" lower="-1.571" upper="1.571" velocity="0.5"/> -->
    <origin rpy="0 0 0" xyz="5 103 8"/>
  </joint>

  <!-- wheel2 -->
  <link name="wheel2">
