        <mesh filename="wheel.stl" scale="1.00 1.00 1.00"/>
      </geometry>
    </collision>
  </link>

  <joint name="joint2" type="continuous">
    <parent link="base_link"/>
    <child link="wheel2"/>
    <axis xyz="0 1 0"/>
    <!-- <limit effort="1000.0" lower="-1.571" upper="1.571" velocity="0.5"/> -->
    <origin rpy="0 0 0" xyz="135 118 8"/>
  </joint>

  <!-- wheel3 -->
